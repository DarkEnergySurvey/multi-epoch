universe=local
executable=$(deshome)/libexec/desingestsrc
arguments = $(descfg) $(Cluster)
getenv=true
notification=never
output=ingestsrc.$(Cluster).$(Process).out
error=ingestsrc.$(Cluster).$(Process).err
log=ingestsrc.log
+des_isdesjob=TRUE
+des_nite="$(nite)"
+des_runid="$(runid)"
+des_runsite="$(runsite)"
+des_stage="ingestsrc"
+des_substage=" "
queue
