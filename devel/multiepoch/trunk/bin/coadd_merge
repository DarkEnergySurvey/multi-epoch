#!/usr/bin/env python

import time
import logging
from multiepoch import coadd_merge
from despymisc.miscutils import elapsed_time

# Get the start time
t0 = time.time()
args = coadd_merge.cmdline()
kwargs = vars(args)
coadd_merge.merge(**kwargs)
logger = logging.getLogger('Merge')
logger.info("Coadd Merge Time: %s" % elapsed_time(t0))
